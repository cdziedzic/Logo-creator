{
  "dependencies": {
    "add": "^2.0.6",
    "inquirer": "^8.0.0",
    "inquirer-maxlength-input-prompt": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "babel-jest": "^29.6.3",
    "cross-env": "^7.0.3",
    "jest": "^29.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "mjs",
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "testMatch": [
      "**/?(*.)test.?js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.m?js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "\\.pnp\\.[^\\/]+$"
    ]
  },
  "scripts": {
    "test": "jest"
  }
}
